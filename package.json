{
  "name": "ts-esm-project-template",
  "version": "1.0.0",
  "description": "Modern TypeScript project template",
  "keywords": [
    "typescript",
    "modules",
    "esm",
    "esnext"
  ],
  "license": "MIT",
  "author": "dandv",
  "type": "module",
  "main": "run.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -f *.js *.js.map",
    "lint": "eslint *.ts",
    "start": "node --experimental-specifier-resolution=node -r source-map-support/register run.js",
    "test": "node node_modules/.bin/jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-plugin-jest": "^23.18.2",
    "jest": "^26.1.0",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
